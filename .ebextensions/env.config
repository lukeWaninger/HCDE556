container_commands:
    01-mkdir:
        command: "mkdir ~/../../var/app/current/data"
        ignoreErrors: true
    02-get-data:
        command: "aws s3 cp s3://elasticbeanstalk-us-east-1-701856502070/RedditJokes/data/ /home/ec2-user --recursive"
    03-mv-to-src:
        command: "cp -a /home/ec2-user/. ~/../../var/app/current/data/"

option_settings:
    aws:elasticbeanstalk:container:nodejs:staticfiles:
        /css: staticcss
        /data: staticdata
        /js: staticjs